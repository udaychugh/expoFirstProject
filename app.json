{
  "expo": {
    "name": "Life Match",
    "slug": "life-match",
    "version": "2026.0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.freelabtech.lifematch",
      "usesCleartextTraffic": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "277f3f87-f40a-4cae-8ec0-31fd4163c88d"
      }
    }
  }
}
